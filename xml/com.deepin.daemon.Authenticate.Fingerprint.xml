       <interface name="com.deepin.daemon.Authenticate.Fingerprint">
    <property name="DefaultDevice" type="s" access="read"></property>
    <property name="Devices" type="s" access="read"></property>	

    <method name="SetDefaultDevice">
        <arg name="device" type="s" direction="in"/>
    </method>

    <method name="Claim">
	    <arg type="s" name="id" direction="in"/>
	    <arg type="b" name="claimed" direction="in"/>
    </method>
    <method name="Enroll">
	    <arg type="s" name="id" direction="in"/>
	    <arg type="s" name="finger" direction="in"/>
    </method>
    <method name="CancelEnroll">
	    <arg type="s" name="id" direction="in"/>
    </method>
    <method name="DeleteFinger">
	    <arg type="s" name="id" direction="in"/>
	    <arg type="s" name="finger" direction="in"/>
    </method>
    <method name="DeleteAllFingers">
	    <arg type="s" name="id" direction="in"/>
    </method>
    <method name="ListFingers">
	    <arg type="s" name="id"  direction="in"/>
	    <arg type="as" name="fingers" direction="out"/>
    </method>
    <method name="StopEnroll">
    </method>

    <signal name="EnrollStatus">
            <arg type="s" name="id"/>
	    <arg type="i" name="code"/>
            <arg type="s" name="msg"/>
    </signal>
    <signal name="Touch">
	    <arg type="s" name="id"/>
	    <arg type="b" name="pressed"/>
    </signal>
  </interface>
